{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "import/extensions": "off",
        "class-methods-use-this": "off",
        "no-alert": "off",
        "no-console": "off",
        "import/no-named-as-default-member": "off",
        "import/no-named-as-default": "off",
        "import/prefer-default-export": "off"
    }
}